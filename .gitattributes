# windows files - 
.bat            text eol=crlf
.cmd            text eol=crlf
.ps1            text eol=crlf

# These files are text and should be normalized (Convert crlf => lf)
*.bash          text eol=lf
*.sh            text eol=lf
mvnw            text eol=lf
gradelw         text eol=lf
*.properties    text eol=lf
*.css           text diff=css
*.htm           text diff=html
*.html          text diff=html
*.java          text diff=java
